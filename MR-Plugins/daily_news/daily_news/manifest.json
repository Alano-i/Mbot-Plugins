{
  "name": "daily_news",
  "title": "每天60秒读懂世界",
  "author": "Alano",
  "description": "获取每日新闻和天气",
  "version": "0.1",
  "requirements": [],
  "configField": [
    {
      "fieldName": "uid",
      "fieldType": "Enum",
      "label": "推送用户",
      "helperText": "选择接收推送的用户,需在用户管理页独立设置推送配置",
      "enumValuesRef": "UserList",
      "multiValue": true
    },
    {
      "fieldName": "city",
      "fieldType": "String",
      "label": "设置城市，用于查询天气，例: 北京",
      "helperText": "",
      "defaultValue": "北京"
    },
    {
      "fieldName": "key",
      "fieldType": "String",
      "label": "和风天气api key",
      "helperText": "在 https://dev.qweather.com 申请 key，创建项目后进入控制台新建项目然后添加 key",
      "defaultValue": ""
    },
    {
      "fieldName": "wecom_proxy_url",
      "fieldType": "String",
      "label": "微信白名单代理设置，示例: https://abc.com",
      "helperText": "",
      "defaultValue": ""
    },
    {
      "fieldName": "qywx_channel_extra",
      "fieldType": "Bool",
      "label": "启用独立微信应用推送消息",
      "helperText": "开启此项需完成下面4个设置项，上方第一个设置项选择的推送用户将会失效。反之关闭则下方设置项失效。参数获取参考：https://alanoo.notion.site/thumb_media_id-64f170f7dcd14202ac5abd6d0e5031fb",
      "defaultValue": false
    },
    {
      "fieldName": "corpid_extra",
      "fieldType": "String",
      "label": "企业 corpid，启用独立推送此设置生效",
      "helperText": "",
      "defaultValue": ""
    },
    {
      "fieldName": "corpsecret_extra",
      "fieldType": "String",
      "label": "微信应用 corpsecret，启用独立推送此设置生效",
      "helperText": "",
      "defaultValue": ""
    },
    {
      "fieldName": "agentid_extra",
      "fieldType": "String",
      "label": "微信应用 agentid，启用独立推送此设置生效",
      "helperText": "",
      "defaultValue": ""
    },
    {
      "fieldName": "touser_extra",
      "fieldType": "String",
      "label": "微信消息推送接收人 touser，启用独立推送此设置生效",
      "helperText": "",
      "defaultValue": ""
    }
  ],
  "logoUrl": "/plugins/daily_news/logo.jpg",
  "githubUrl": "https://github.com/Alano-i/wecom-notification/tree/main/MR-Plugins/daily_news",
  "helpDocUrl": "https://github.com/Alano-i/wecom-notification/tree/main/MR-Plugins/daily_news",
  "dependencies": null
}
